{
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "playwright test --config=playwright.config.ts",
    "test:debug": "playwright test --config=playwright.config.ts --debug",
    "test:headless": "HEADLESS=true playwright test --config=playwright.config.ts",
    "test:slow": "PLAYWRIGHT_SLOW_MO=300 playwright test --config=playwright.config.ts",
    "test:actions": "TEST_ACTIONS=true playwright test --config=playwright.config.ts",
    "codegen:lucy": "nodemon --exec \"lucyc src/machine/machine.lucy --out-file src/machine/lucy.machine.js\" src/machine/machine.lucy",
    "deploy": "sh deploy.sh",
    "typegen": "xstate typegen \"./src/**/*.ts?(x)\"",
    "typegen:watch": "xstate typegen \"./src/**/*.ts?(x)\" --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@playwright/test": "^1.29.1",
    "@statelyai/inspect": "^0.2.3",
    "@xstate/graph": "^1.4.2",
    "@xstate/immer": "^0.3.1",
    "@xstate/react": "^4.1.0",
    "@xstate/test": "^0.5.1",
    "@xstate/vue": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^3.2.45",
    "xstate": "^5.7.1",
    "xstate4": "npm:xstate@^4.35.1"
  },
  "devDependencies": {
    "@lucy/rollup-plugin": "^0.1.4",
    "@storybook/addon-essentials": "^7.0.0-beta.31",
    "@storybook/addon-interactions": "^7.0.0-beta.31",
    "@storybook/addon-links": "^7.0.0-beta.31",
    "@storybook/blocks": "^7.0.0-beta.31",
    "@storybook/react": "^7.0.0-beta.31",
    "@storybook/react-vite": "^7.0.0-beta.31",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@xstate/cli": "^0.4.2",
    "fast-check": "^3.6.2",
    "nodemon": "^3.0.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.0-beta.31",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vue-tsc": "^1.0.24"
  }
}
